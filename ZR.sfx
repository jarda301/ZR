// ENGINE THROTTLE-POSITION MIX
EngineLoadBlendInside=(0.3,0.7)   // power sound starts to blend in at 30% throttle, coast sound blends out at 70%
EngineLoadBlendOutside=(0.3,0.7)  // power sound starts to blend in at 30% throttle, coast sound blends out at 70%


// ENGINE REV-SAMPLE MIX
// RPMs need to overlap properly from one sample to the next (maximum of two playing at once).
// The first minimum RPM should be above zero but low enough to hear an engine stall.

// coast inside
EngRPMCoastInside=(0,     1.00,  2671.00,  1500.00) // zero-based engine number, minimum RPM, maximum RPM, natural RPM at which sample was recorded
EngRPMCoastInside=(1,  1560.00,  5534.00,  4000.00)
EngRPMCoastInside=(2,  4380.00,  7799.00,  6000.00)
EngRPMCoastInside=(3,  6624.00, 10000.00,  8000.00)

// power inside
EngRPMPowerInside=(0,     1.00,  2671.00,  1500.00) // zero-based engine number, minimum RPM, maximum RPM, natural RPM at which sample was recorded
EngRPMPowerInside=(1,  1560.00,  5534.00,  4000.00)
EngRPMPowerInside=(2,  4380.00,  7799.00,  6000.00)
EngRPMPowerInside=(3,  6624.00, 10000.00,  8000.00)

// coast outside
EngRPMCoastOutside=(0,     1.00,  2863.00,  1500.00) // zero-based engine number, minimum RPM, maximum RPM, natural RPM at which sample was recorded
EngRPMCoastOutside=(1,  1688.00,  4551.00,  3000.00)
EngRPMCoastOutside=(2,  3782.00,  6603.00,  5000.00)
EngRPMCoastOutside=(3,  5556.00, 10000.00,  7000.00)

// power outside
EngRPMPowerOutside=(0,     1.00,  2863.00,  1500.00) // zero-based engine number, minimum RPM, maximum RPM, natural RPM at which sample was recorded
EngRPMPowerOutside=(1,  1688.00,  4551.00,  3000.00)
EngRPMPowerOutside=(2,  3782.00,  6603.00,  5000.00)
EngRPMPowerOutside=(3,  5556.00, 10000.00,  7000.00)

